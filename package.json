{
  "name": "reactexpress",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "client": "npm start --prefix client",
    "server": "nodemon server.js",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "concurrently": "^3.5.1",
    "express": "^4.16.2",
    "google-map-react": "^0.34.0"
  }
}
